{
	"name": "Machine Learning Name Finder",
	"version": "0.2",
	"permissions": [
		"activeTab", "*://*.students.cec.wustl.edu/~cse517a/*"
 	],
 	"background": {
		"matches": ["*://*.students.cec.wustl.edu/~cse517a/*"],
		"scripts": ["background.js"],
		"persistent": true
 	},
 	"browser_action": {
		"default_title": "Allows you to easily find your line in the matrix.",
		"default_icon": "terminator.png",
		"default_popup": "popup.html"
 	},
 	"manifest_version": 2
 	}